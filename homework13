using System;

class Program
{
    static void Main()
    {
        PC pc = new PC();
        pc.cpu = "Intell";
        pc.os = "Windows";
        pc.weight = 15;
        pc.timeAW = 1;
        pc.cores = 6;
        
        Console.WriteLine(pc.cpu);
        Console.Read();
    }
}

